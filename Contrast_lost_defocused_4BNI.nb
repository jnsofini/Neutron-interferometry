Cell[TextData[{
 "The intensity at the exit of a defocused three-blade NI is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"I", "=", 
    RowBox[{
     RowBox[{"\[Integral]", "2"}], "|", "trr", 
     SuperscriptBox["|", "2"], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"cos", "(", 
         RowBox[{"\[Phi]", "+", 
          RowBox[{"2", 
           RowBox[{"\[Pi]\[Delta]y", "/", "\[CapitalDelta]"}]}]}], ")"}]}], 
       ")"}], "dy"}]}]}], TraditionalForm]],ExpressionUUID->
  "af338e4d-e7f2-4673-9495-5dfd9aa58e88"],
 " where the integral is taken over the angular deviation parameter \
y=\[Delta]\[Theta]/\[Sigma];"
}], "Text",
 CellChangeTimes->{{3.68890891407843*^9, 
  3.688909084963398*^9}},ExpressionUUID->"4a98856f-8ddc-4fe6-a30e-\
c96c77d4abeb"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"IH", "[", "x_", "]"}], ":=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{
     FractionBox["16", 
      RowBox[{"45", "\[Pi]"}]], 
     FractionBox[
      RowBox[{
       RowBox[{"40", 
        SuperscriptBox["y", "2"]}], "+", "1"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "+", 
         SuperscriptBox["y", "2"]}], ")"}], "4"]], 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"Cos", "[", " ", 
        RowBox[{"2", "\[Pi]", " ", "x", " ", "y"}], "]"}]}], ")"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"y", ",", 
      RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}]}], "]"}], 
  "  ", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"x", "=", 
     RowBox[{"\[CapitalDelta]z", "/", 
      SubscriptBox["\[CapitalDelta]", "H"]}]}], ",", " ", 
    RowBox[{
    "that", " ", "is", " ", "the", " ", "defocusing", " ", "over", " ", "the",
      " ", "Pendellosung", " ", "period"}]}], "*)"}]}]], "Input",
 CellChangeTimes->{{3.688907390358081*^9, 3.688907401746152*^9}, {
   3.688907470748054*^9, 3.688907473127182*^9}, {3.688907504772678*^9, 
